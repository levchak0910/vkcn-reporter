{
  "name": "vkcn-report-duplicated-class-selectors",
  "version": "0.1.5",
  "description": "This tool helps to detect element classes definition duplication",
  "type": "module",
  "main": "./dist/program.js",
  "bin": "./dist/cli.js",
  "types": "./dist/program.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:levchak0910/vkcn-report-duplicated-class-selectors.git"
  },
  "scripts": {
    "test": "vitest run",
    "build": "rimraf dist && tsc",
    "lint": "prettier . --check",
    "fix": "prettier . --write",
    "cli": "pnpm build && node ./dist/cli.js",
    "prepare": "husky install"
  },
  "keywords": [
    "vue",
    "eslint-plugin-vue-kebab-class-naming",
    "duplicate",
    "class name"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@csstools/postcss-extract": "^3.0.0",
    "glob": "^9.2.1",
    "make-cli": "^3.0.4",
    "postcss": "^8.4.21",
    "postcss-scss": "^4.0.6",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "eslint": "^8.35.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "typescript": "^4.9.5",
    "vitest": "^0.32.2"
  }
}
