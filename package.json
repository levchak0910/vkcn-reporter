{
  "name": "@vkcn/reporter",
  "version": "0.2.3",
  "description": "Tool that helps to detect VKCN violations across the whole project",
  "type": "module",
  "main": "./dist/program.js",
  "bin": {
    "vkcn-reporter": "./dist/cli.js"
  },
  "types": "./dist/program.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:levchak0910/vkcn-reporter.git"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "rimraf dist && tsc",
    "lint": "prettier . --check",
    "fix": "prettier . --write",
    "cli": "pnpm build && node ./dist/cli.js",
    "prepare": "husky",
    "release": "bumpp package.json --commit --push --tag"
  },
  "keywords": [
    "vue",
    "@vkcn/eslint-plugin",
    "classes",
    "class-naming",
    "vkcn",
    "kebab-case"
  ],
  "author": "Levcsák Sándor",
  "license": "MIT",
  "dependencies": {
    "@csstools/postcss-extract": "^3.0.1",
    "citty": "^0.1.6",
    "glob": "^10.3.12",
    "postcss": "^8.4.38",
    "postcss-scss": "^4.0.9",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@types/node": "^20.12.2",
    "bumpp": "^9.4.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.3",
    "vitest": "^1.4.0"
  }
}
