    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@types/node": "^20.12.2",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "make-cli": "^4.0.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.3",
    "vitest": "^1.4.0"
